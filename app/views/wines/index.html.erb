<h1 class="text-center">Wine List</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Appellation</th>
      <th>Varietal</th>
      <th>Vineyard</th>
    </tr>
  </thead>
  <tbody class="table-hover">

    <% @wines.each do |wine| %>
      <%= render wine %>
    <% end %>

  </tbody>
</table>
